<script>
  // if there is a campaign field, populate it with the content of the cookie
  $(document).ready(function() {

      // update abtest fields

      var test_buckets = ["A", "B"]
      var bucket = getCookie("AB_test");

      if (!bucket) {
          var rnum = Math.floor(Math.random() * 2);
		  bucket = test_buckets[rnum]
		  console.log("resetting cookie = " + bucket);
      }

      if (query_params["seats"]) {
          var seats = query_params["seats"];
          if (seats < 2) {
            $( ".free_license" ).addClass( "hide" );
          } else {
            $( ".free_license" ).removeClass("hide");
          }

          if ($('.cart').length > 0) {
              var seats = query_params["seats"];
              showproduct(seats);
          }
      }

	  if (typeof(muse_init) != "undefined") {
		  muse_init = (function() {
			//console.log("overriding muse_init()");
			var cached_function = muse_init;
			
			return function() {
			  setTimeout(function(){ 
				  if (!($("body").hasClass("initialized"))) {
					  ga('send', 'event', 'Muse_load_failed_5', window.location.href);
				  }
			  }, 3500);
			  cached_function.apply(this, arguments); // use .apply() to call it
			};
		  }());
	  }
	});
</script>

