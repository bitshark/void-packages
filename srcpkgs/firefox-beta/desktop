<script>

  // parses out the query parameters from the url
  var query_params = (function(a) {
    if (a == "") return {};
    var b = {};
    for (var i = 0; i < a.length; ++i)
    {
        var p=a[i].split('=', 2);
        if (p.length == 1)
            b[p[0]] = "";
        else
            b[p[0]] = decodeURIComponent(p[1].replace(/\+/g, " "));
    }
    return b;
  })(window.location.search.substr(1).split('&'));


  // gets the cookie with the given name
  function getCookie(name) {
    match = document.cookie.match(new RegExp(name + '=([^;]+)'));
    if (match) return match[1];
  };


  // sets a cookie for 90 days, if not already exists.
  function setCookie(name, value, exp) {
    if (getCookie(name))
        return

     var d = new Date();

     // expires in exp days
     var expires = "";
     if (exp > 0) {
       d.setTime(d.getTime() + (exp*24*60*60*1000));
       expires = "expires="+ d.toUTCString();
     }

     document.cookie = name + "=" + value + ';path=/;' + expires;
     //document.cookie = name + "=" + value + ';path=/;';
  };

  // sets a cookie for 90 days
  function resetCookie(name, value, exp) {
      var d = new Date();

     // expires in exp days
     var expires = "";
     if (exp > 0) {
       d.setTime(d.getTime() + (exp*24*60*60*1000));
       expires = "expires="+ d.toUTCString();
     }

     document.cookie = name + "=" + value + ';path=/;' + expires;
  };


  // if the cmp parameter is on the url, set a cookie
  if (query_params["cmp"]) {
     console.log("coming from campaign. set cookie: " + query_params["cmp"]);
     setCookie("campaign", query_params["cmp"], 90);
  };

</script>




